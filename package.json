{
    "name": "sap-btp-cloud-logging-client",
    "version": "1.0.1",
    "description": "SAP BTP Cloud Logging client for Node.js applications",
    "main": "index.js",
    "types": "types/index.d.ts",
    "files": [
        "lib",
        "types",
        "index.js",
        "README.md",
        "LICENSE"
    ],
    "publishConfig": {
